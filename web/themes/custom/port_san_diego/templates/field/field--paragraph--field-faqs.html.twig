{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
    'field--type-' ~ field_type|clean_class,
    'field--label-' ~ label_display,
    'faqs__questions',
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {% for item in items %}
    <div{{ item.attributes.addClass('faqs__question-block') }}>{{ item.content }}</div>
  {% endfor %}
</div>
