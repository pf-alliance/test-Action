{% extends 'node.html.twig' %}
{#
/**
 * @file
 * Theme override to display a node.
 */
#}
{% block content %}
  {{ title_prefix }}
  {{ title_suffix }}
  <a href="{{ url }}" class="taskbar__link" {{ link_attributes }}>
    <svg class="taskbar__task-icon icon icon--{{ icon }}" alt="{{ label.0  }}">
      <use xlink:href="#icon-{{ icon }}"></use>
    </svg>
    <span class="taskbar__task-name">{{ label }}</span>
  </a>
{% endblock %}
